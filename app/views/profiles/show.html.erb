
<div class="wrapper hive-bg">
  <div class="container">
     <div class="row" >
        <div class='col-xs-12 col-md-6'>
           <h1><%= @profile.first_name %></h1>
           <h3><%= @profile.city %> </h3>
           <h2> About Me </h2>
           <p><%= @profile.interest%> </p>
           <h3>Upcoming Meals</h3>

           <h4>Countries i have Visited...</h4>
           <p> <%=@profile.visited%></p>
           <h4> Countries i Want to visit...</h4>
           <p><%=@profile.future_visit%></p>
        </div>
       <div class='col-xs-12 col-md-6' class="img">
        <%= image_tag @profile.photo %>
       </div>
     </div>
   </div>

</div>
<div class="edit-btn">
 <%= link_to ("edit your profile"), edit_profile_path, class: "dashboard-button"
 %></div>










